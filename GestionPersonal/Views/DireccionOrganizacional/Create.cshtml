@model GPDataInformation.Models.DireccionOrganizacional

@{
    ViewData["Title"] = "Create";
}


<div class="panel panel-default">
    <div class="panel-heading clearfix">
        <h4 class="panel-title pull-left" style="padding-top: 7.5px;">  @ViewData["Title"]</h4>
        <div class="btn-group pull-right">

        </div>
    </div>
    <div class="panel-body" id="list_departammets">
        <div class="panel-body" style="min-height: 214px;">
            <form class="form-horizontal" asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Nombre" class="control-label col-sm-2"></label>
                    <div class="col-sm-10">
                        <input asp-for="IdDireccion" type="hidden" class="form-control" />
                        <input asp-for="Nombre" class="form-control" />
                        <span asp-validation-for="Nombre" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <label asp-for="IdSociedad" class="control-label col-sm-2"></label>
                    <div class="col-sm-10">
                        <select asp-for="IdSociedad" class="form-control" asp-items="ViewBag.Sociedades">
                            <option value="">Selecciona una opcion</option>
                        </select>
                        <span asp-validation-for="IdSociedad" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group" style="margin-bottom:0">
                    <div class="col-sm-offset-2 col-sm-10 text-right">
                        <a asp-action="Index" class="btn btn-default" title="Cancelar">Cancelar</a>
                        <button type="submit" class="btn btn-primary" title="Guardar cambios">Guardar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
